/* header 部分导航 */

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.active {
  color: #d81e06;
  border-bottom: 2rpx solid #d81e06;
  font-size: 32rpx;
  margin-left: 2rpx;
  margin-right: 2rpx;
}

.articles {
  background-color: #fff;
}

.article__item {
  display: flex;
  padding: 30rpx;
  align-items: center;
  border-bottom: 1rpx solid #eee;
  position: relative;
  padding-bottom: 40rpx;
}

.article__thumb {
  width: 4.2rem;
  height: 3rem;
  margin-right: 0.6rem;
}

.article__image {
  width: 4.2rem;
  height: 3rem;
}

.article__info {
  flex: 1;
  width: 100%;
  overflow: hidden;
}

.article__title {
  font-weight: 400;
  font-size: 26rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block;
  width: 100%;
  margin-bottom: 20rpx;
}

.article__desc {
  color: #999;
  font-size: 24rpx;
  line-height: 1.2;
  width: 100%;
}

.article__item__cover {
  display: block;
}

.article__cover {
  padding-bottom: 10rpx;
  overflow: hidden;
}

.article__ad {
  width: 100%;
  height: 4rem;
  border-radius: 10rpx;
}

.article__remark {
  bottom: 0;
  width: 20rem;
  display: block;
  font-size: 28rpx;
}

.article__source {
  color: #d81e06;
  border: 1rpx solid #d81e06;
  padding: 2rpx 10rpx;
  border-radius: 4rpx;
  font-size: 20rpx;
  margin-right: 20rpx;
}

.article__time {
  margin-right: 10rpx;
}

.article__count {
  float: right;
  display: flex;
  align-items: center;
  color: #333;
  font-size: 20rpx;
  font-weight: 500;
}

.article__icon {
  width: 29rpx;
  height: 29rpx;
  margin-left: 15rpx;
  margin-right: 6rpx;
}

.article__imgview {
  width: 200rpx;
  height: 142rpx;
  padding: 15rpx;
}

.body_YIhtSk {
  background-color: rgb(0, 0, 0);
  height: 100%;
  position: relative;
  width: 100%;
  overflow: hidden;
}

.header {
  width: 100%;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  height: 64rpx;
  position: relative;
}

.sv__container {
  width: 100%;
  line-height: 64rpx;
  white-space: nowrap;
  background-color: #fff;
}

.sv__item {
  margin-top: 10rpx;
  padding: 24rpx 24rpx 0 24rpx;
  display: inline-block;
  text-align: center;
}

.swiper__container {
  height: 10rem;
}

.swiper__desc {
  position: absolute;
  width: 100%;
  bottom: 0;
  padding: 10rpx;
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.swiper__item {
  display: block;
  width: 100%;
  height: 10rem;
}

.swiper__id {
  float: right;
  padding-right: 20rpx;
  font-weight: 700;
}

/* 基础样式 */

view, scroll-view, swiper, swiper-item, icon, text, progress, button,
checkbox-group, checkbox, form, input, label, picker, radio-group, radio, slider,
switch, action-sheet, action-sheet-item, action-sheet-cancel, modal, toast,
loading, navigator, audio, image, video, map, canvas {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

page {
  height: 100%;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

checkbox-group, radio-group {
  display: block;
}

:before, :after, ::before, ::after {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.c-minheight {
  min-height: 80px;
}

.c-full {
  width: 100%;
  height: 100%;
}

.c-block {
  display: block;
}

.c-autosize {
  width: auto;
  height: auto;
}

.c-fullwidth {
  width: 100%;
}

.c-fullheight {
  height: 100%;
}

.c-initHide {
  opacity: 0 !important;
}

.c-ellipse {
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

